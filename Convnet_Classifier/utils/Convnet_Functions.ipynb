   "source": [
    "import numpy as np\n",
    "\n",
    "def Calc_Dims_3d(shape, padding, kernel, stride, Pool_kernel, Pool_stride):\n",
    "    convshape = np.round((shape + 2*padding - kernel) / stride, 0)\n",
    "    poolshape = np.round((convshape - Pool_kernel) / Pool_stride, 0)\n",
    "    \n",
    "    return poolshape"
   ]
  }
